<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hhcf.learn.dao.FullMybatisDao">
	<select id="findUsers" parameterMap="" parameterType=""
		resultType="com.hhcf.learn.model.UserModel">
		select user_id id,user_name userName,user_age age from t_user where user_id=#{id}


	</select>
</mapper>
